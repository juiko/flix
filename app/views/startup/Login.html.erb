<script>
  $(document).ready(function() {
    $('#success-message').hide();
    $('#error-message').hide();
  
    $('#save-notifications-button').click(function() {
      $(this).attr('disabled', true);
      $(this).text('Saving...');
      $('#success-message').hide();
      $('#error-message').hide();
  
      $.ajax({
        type: 'POST',
        url: '/users/save_user_notifications',
        data:  { setting: $('input[type=radio]:checked', '#notification-settings-container').val()},
        success: function() {
          $('#save-notifications-button').attr('disabled', false);
          $('#save-notifications-button').text('Save');
          $('#success-message').show();
        },
        error: function(resp) {
          $('#save-notifications-button').attr('disabled', false);
          $('#save-notifications-button').text('Save');
          $('#error-message').html(JSON.parse(resp.responseText).error_message);
          $('#error-message').show();
        }
      });
    });
  
    $('#close-notifications').click(function() {
      $('#notifications-modal').modal('hide');
    });
  
    $('#notifications-modal').on('hide', function() {
      $('#success-message').hide();
      $('#error-message').hide();
    });
  
  
  });
</script>
