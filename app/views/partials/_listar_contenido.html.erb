<% @contenido.each do |weaita| %>

  <div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail">
      <img src="http://placehold.it/320x150" alt="">

      <div class="caption">
	<h4 class="pull-right">
	  <%= weaita.klass %>
	</h4>
	<h4>
	  <%= link_to weaita.normalized_title, weaita %>
	</h4>
      </div>
      <div class="ratings">

	<p class="pull-right">15 reviews</p>
	<p>
	  <span class="glyphicon glyphicon-star"></span>

	  <span class="glyphicon glyphicon-star"></span>

	  <span class="glyphicon glyphicon-star"></span>
	  <span class="glyphicon glyphicon-star"></span>

	  <span class="glyphicon glyphicon-star"></span>
	</p>
      </div>
    </div>
  </div>

<% end %>
